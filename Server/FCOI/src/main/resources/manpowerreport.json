	"extraCondition" : "(LEAD_UNIT_NUMBER IN(SELECT DISTINCT UNIT_NUMBER FROM PERSON_ROLE_RT WHERE ((RIGHT_NAME IN ('MANPOWER_VIEW_STAFF_PLAN') AND MANPOWER_TYPE_CODE = '1') OR (RIGHT_NAME IN ('MANPOWER_VIEW_STUDENT_PLAN') AND MANPOWER_TYPE_CODE = '2') OR (RIGHT_NAME IN ('MANPOWER_VIEW_OTHERS_PLAN') AND MANPOWER_TYPE_CODE = '3')) AND PERSON_ID =<<PERSON_ID>>) OR (AWARD_ID IN (SELECT T1.AWARD_ID FROM AWARD_PERSON_ROLES T1 INNER JOIN ROLE_RIGHTS T2 ON T1.ROLE_ID = T2.ROLE_ID INNER JOIN RIGHTS T3 ON T2.RIGHT_ID = T3.RIGHT_ID WHERE T1.PERSON_ID =<<PERSON_ID>> AND ((T3.RIGHT_NAME IN ('MANPOWER_VIEW_STAFF_PLAN') AND MANPOWER_TYPE_CODE = '1') OR (T3.RIGHT_NAME IN ('MANPOWER_VIEW_STUDENT_PLAN') AND MANPOWER_TYPE_CODE = '2') OR (T3.RIGHT_NAME IN ('MANPOWER_VIEW_OTHERS_PLAN') AND MANPOWER_TYPE_CODE = '3')))))",
