{
  "name": "fibi",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 4500 --host 0.0.0.0",
    "start-dynamic": "ng serve --proxy-config proxy.conf.json --configuration=label --port 4500",
    "create-labels": "ng xi18n --output-path labels",
    "buildSSO": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration production --deploy-url /fibi-sso/resources/dist/ --configuration=label",
    "build": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration=label",
    "stats": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "verify-build": "webpack-bundle-analyzer dist/stats-es2015.json",
    "source": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration production --source-map",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.2",
