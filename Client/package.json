    "element-remove": "^1.0.4",
    "install": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.27.0",
    "npm": "^6.14.7",
    "rxjs": "^6.6.2",
    "socket.io-client": "^4.3.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.11.4"
